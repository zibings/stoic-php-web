{
	"name": "stoic/web",
	"description": "Web site helper components for the Stoic framework",
	"version": "1.0.0",
	"homepage": "https://stoic-framework.com",
	"autoload": {
		"classmap": ["Web/"],
		"exclude-from-classmap": [
			"Tests/"
		]
	},
	"repositories": [
		{
			"url": "https://github.com/zibings/stoic-php-core.git",
			"type": "git"
		},
		{
			"url": "https://github.com/zibings/stoic-php-io.git",
			"type": "git"
		},
		{
			"url": "https://github.com/zibings/stoic-php-pdo.git",
			"type": "git"
		}
	],
	"require": {
		"php": "^7.2",
		"stoic/stoic": "dev-master",
		"stoic/io": "dev-master",
		"stoic/pdo": "dev-master",
		"andym84/config": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7"
	},
	"scripts": {
		"post-create-project-cmd": [
			"stoic-create"
		]
	},
	"bin": [
		"stoic-create"
	]
}
